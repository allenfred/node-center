    "start": "nodemon --exec ts-node ./src/main.ts",
    "dev": "npm run clean && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenfred/quant-wsserver.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allenfred/quant-wsserver/issues"
  },
  "homepage": "https://github.com/allenfred/quant-wsserver#readme",
  "dependencies": {
    "@binance/connector": "^1.8.1",
    "@okfe/okex-node": "0.0.52",
    "bluebird": "^3.5.5",
    "jest": "^24.9.0",
    "log4js": "^5.1.0",
    "moment": "^2.24.0",
    "mongoose": "^5.11.15",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.16",
    "redis": "^4.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.9.10",
    "winston": "^3.2.1",
    "ws": "^7.5.8"
  },
  "devDependencies": {
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.20.52",
    "typescript": "^3.6.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
