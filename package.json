{
  "name": "node-center",
  "version": "0.1.0",
  "description": "node server for crypto",
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "local": "dotenv -e .env.local nodemon --exec ts-node ./src/main.ts",
    "start": "nodemon --exec ts-node ./src/main.ts",
    "dev": "npm run clean && npm run build && npm run start"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@binance/connector": "^1.8.1",
    "@okfe/okex-node": "0.0.52",
    "bluebird": "^3.5.5",
    "bybit-api": "^2.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.21",
    "log4js": "^5.1.0",
    "moment": "^2.24.0",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "nodemon": "^2.0.16",
    "redis": "^4.1.0",
    "socket.io": "^4.5.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.9.10",
    "winston": "^3.2.1",
    "ws": "^7.5.8"
  },
  "devDependencies": {
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.20.52",
    "typescript": "^3.6.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "json",
      "ts",
      "js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  }
}
